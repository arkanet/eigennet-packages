config section general
	option	'debugLevel'		'0'
	option	'bootmode'		'0'		# 0 First boot, 1 Run Autoconfig, 2 Auto Configured, 3 or greater added custom configuration.

config section network
	option	'accept_clients'	'1'
	option	'ip6gw'			'2001:470:c8f7:ee:ee::1000'	# ipv6 gateway
	option	'mesh6Prefix'		'2001:470:c8f7:ee:ee:'		# /80 prefix
	option	'firewallEnabled'	'0'
	list	'resolvers'		'2001:470:ca42:ee:ab::1000 8.8.8.8'
	option	'sshEigenserverKey'	'ssh-rsa  www-data@eigenserver'

config section wireless
	option	'ath9k_clients'		'0'
	option	'ath9k_mesh'		'1'
	option	'madwifi_clients'	'0'
	option	'madwifi_mesh'		'1'
	option	'mesh2channel'		'8'
	option	'mesh5channel'		'60'
	option	'meshSSID'		'www.ninux.org'
	option	'meshBSSID'		'02:aa:bb:cc:dd:ee'
	option	'apSSID'		'NebrodiNet'
	option  'apKEY'                 'nebrodi'

config section wired
	option	'eth_mesh'		'1'
	option	'eth_clients'		'1'
